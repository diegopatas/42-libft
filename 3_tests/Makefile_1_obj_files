all: main.o ft_sum.o
	gcc -Wall -Wextra -Werror main.o ft_sum.o -o program

main.o: main.c lib.h
	gcc -c main.c

ft_sum.o: ft_sum.c lib.h
	gcc -c ft_sum.c

clean:
	rm -rf main.o ft_sum.o program
